        <arguments>
          <argument>
            <kind>1</kind>
            <string>

next = ds_grid_create(ds_grid_width(tablero), ds_grid_height(tablero));
recientes = ds_grid_create(ds_grid_width(tablero), ds_grid_height(tablero));


for (var i=0; i&lt;c; i++){
    for(var j=0; j&lt;f; j++){
    
    
     var state = ds_grid_get(tablero,i,j);
     
    
    //contar vecinos
    var sum = 0;
   
    
       for (var k=-1; k&lt;2; k++){
    for(var h=-1; h&lt;2; h++){
    
    var col = (i + k + c )mod c;
    var row =(j + h + f) mod f;
    
    sum+=ds_grid_get(tablero,col,row);
    }
}
        sum-=ds_grid_get(tablero,i,j);
            
            
    
    if (state == 0 &amp;&amp; sum ==3){
   ds_grid_set(next,i,j,1);
   ds_grid_set(recientes,i,j,1);
   
   
    }else
    if (state == 1 &amp;&amp; (sum &lt;2 || sum&gt;3)){
    ds_grid_set(next,i,j,0)
    ds_grid_set(recientes,i,j,0)
    }else{
    ds_grid_set(next,i,j,state);
    }
    
   }
}
ds_grid_copy(tablero,next);
ds_grid_clear(next, -1);

if keyboard_check_pressed(ord('R')){
room_restart();

}

</string>
          </argument>
        </arguments>
      </action>
