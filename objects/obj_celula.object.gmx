for (var i=0; i&lt;c; i++){
    for(var j=0; j&lt;f; j++){
    var xx = i*resolution;
    var yy = j*resolution;
   if ds_grid_get(tablero,i,j)=1{
   draw_set_color(col1);
   draw_rectangle(xx,yy,(resolution*i)+resolution,(resolution*j)+resolution,false)
   
         }else{
         draw_set_color(col2);
   draw_rectangle(xx,yy,(resolution*i)+resolution,(resolution*j)+resolution,false)
         }  
    }
}
draw_set_font(Fuente1)
draw_set_color(c_black)
draw_text(700,10,"Press R to play again")



