c = get_integer("Numero de columnas", "");
f = get_integer("Numero de filas", "");



tablero=ds_grid_create(c,f);

for (var i=0; i&lt;c; i++){
    for(var j=0; j&lt;f; j++){
    
    ds_grid_set(tablero,i,j,irandom(1));
    }

}

resolution1=room_height/f
resolution2=room_width/c

if resolution1&gt;=resolution2{

resolution=resolution2
}else{
resolution=resolution1

}



alarm[0]=4;







